{
  "name": "gamelauncher",
  "version": "1.0.0",
  "description": "```\r npm install\r ```\r ### Build\r ```\r npm run build-win\r ```",
  "repository": "git@github.com:aDavroult/game-launcher.git",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "pack": "electron-builder --dir",
    "dist": "electron-builder --x64 --win",
    "gh-publish": "electron-builder --x64 --win -p always",
    "build-win": "electron-packager . gamelauncher --overwrite --asar --icon=icon.ico"
  },
